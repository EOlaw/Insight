<% layout('layouts/boilerplate') %>
<!-- Header Start -->
<div class="container-fluid bg-black py-5 mb-5 page-header">
  <div class="container py-5">
    <div class="row justify-content-center">
      <div class="col-lg-10 text-center">
        <h1 class="display-3 text-white animated slideInDown">Our Expert Team</h1>
        <nav aria-label="breadcrumb">
          <ol class="breadcrumb justify-content-center">
            <li class="breadcrumb-item"><a class="text-white" href="/">Home</a></li>
            <li class="breadcrumb-item text-white active" aria-current="page">Team</li>
          </ol>
        </nav>
      </div>
    </div>
  </div>
</div>
<!-- Header End -->

<!-- Team Intro Section -->
<section id="team-intro" class="py-5">
  <div class="container" data-aos="fade-up">
    <div class="row justify-content-center">
      <div class="col-lg-8 text-center">
        <h2 class="mb-4">Meet the Minds Behind InsightSerenity</h2>
        <p class="lead">Our diverse team of experts brings together years of experience in IT, data science, and business consultancy. We're united by our passion for innovation and our commitment to delivering exceptional results for our clients.</p>
      </div>
    </div>
  </div>
</section>

<!-- Team Members Section -->
<section id="team" class="team py-5 bg-light">
  <div class="container" data-aos="fade-up">
    <div class="row">
      <div class="col-lg-3 col-md-6 mb-4" data-aos="fade-up" data-aos-delay="100">
        <div class="card h-100 shadow-sm">
          <img src="lib/img/team/ceo.jpeg" class="card-img-top" alt="Emmanuel Oyemosu">
          <div class="card-body text-center">
            <h5 class="card-title">Emmanuel Oyemosu</h5>
            <p class="card-text text-muted">Founder & CEO</p>
            <p class="card-text">With over 15 years of experience in IT and data science, Emmanuel leads our team with visionary insight and strategic expertise.</p>
          </div>
          <div class="card-footer bg-transparent border-0">
            <ul class="list-inline mb-0">
              <li class="list-inline-item"><a href="#" class="text-dark"><i class="bi bi-linkedin"></i></a></li>
              <li class="list-inline-item"><a href="#" class="text-dark"><i class="bi bi-twitter"></i></a></li>
              <li class="list-inline-item"><a href="#" class="text-dark"><i class="bi bi-envelope"></i></a></li>
            </ul>
          </div>
        </div>
      </div>

      <div class="col-lg-3 col-md-6 mb-4" data-aos="fade-up" data-aos-delay="200">
        <div class="card h-100 shadow-sm">
          <img src="lib/img/team/team-2.jpg" class="card-img-top" alt="Sarah Johnson">
          <div class="card-body text-center">
            <h5 class="card-title">Sarah Johnson</h5>
            <p class="card-text text-muted">Data Science Lead</p>
            <p class="card-text">Sarah's expertise in machine learning and predictive analytics drives our data-driven solutions for clients across industries.</p>
          </div>
          <div class="card-footer bg-transparent border-0">
            <ul class="list-inline mb-0">
              <li class="list-inline-item"><a href="#" class="text-dark"><i class="bi bi-linkedin"></i></a></li>
              <li class="list-inline-item"><a href="#" class="text-dark"><i class="bi bi-twitter"></i></a></li>
              <li class="list-inline-item"><a href="#" class="text-dark"><i class="bi bi-envelope"></i></a></li>
            </ul>
          </div>
        </div>
      </div>

      <div class="col-lg-3 col-md-6 mb-4" data-aos="fade-up" data-aos-delay="300">
        <div class="card h-100 shadow-sm">
          <img src="lib/img/team/team-3.jpg" class="card-img-top" alt="William Anderson">
          <div class="card-body text-center">
            <h5 class="card-title">William Anderson</h5>
            <p class="card-text text-muted">IT Solutions Architect</p>
            <p class="card-text">William's innovative approach to IT infrastructure and cloud solutions ensures our clients stay ahead in the digital landscape.</p>
          </div>
          <div class="card-footer bg-transparent border-0">
            <ul class="list-inline mb-0">
              <li class="list-inline-item"><a href="#" class="text-dark"><i class="bi bi-linkedin"></i></a></li>
              <li class="list-inline-item"><a href="#" class="text-dark"><i class="bi bi-twitter"></i></a></li>
              <li class="list-inline-item"><a href="#" class="text-dark"><i class="bi bi-envelope"></i></a></li>
            </ul>
          </div>
        </div>
      </div>

      <div class="col-lg-3 col-md-6 mb-4" data-aos="fade-up" data-aos-delay="400">
        <div class="card h-100 shadow-sm">
          <img src="lib/img/team/team-4.jpg" class="card-img-top" alt="Amanda Jepson">
          <div class="card-body text-center">
            <h5 class="card-title">Amanda Jepson</h5>
            <p class="card-text text-muted">Business Strategy Consultant</p>
            <p class="card-text">Amanda's strategic insights and business acumen help our clients navigate complex challenges and achieve sustainable growth.</p>
          </div>
          <div class="card-footer bg-transparent border-0">
            <ul class="list-inline mb-0">
              <li class="list-inline-item"><a href="#" class="text-dark"><i class="bi bi-linkedin"></i></a></li>
              <li class="list-inline-item"><a href="#" class="text-dark"><i class="bi bi-twitter"></i></a></li>
              <li class="list-inline-item"><a href="#" class="text-dark"><i class="bi bi-envelope"></i></a></li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

<!-- Team Values Section -->
<section id="team-values" class="py-5">
  <div class="container" data-aos="fade-up">
    <h2 class="text-center mb-5">Our Team Values</h2>
    <div class="row">
      <div class="col-md-4 mb-4" data-aos="fade-up" data-aos-delay="100">
        <div class="text-center">
          <i class="bi bi-lightbulb text-primary" style="font-size: 3rem;"></i>
          <h4 class="mt-3">Innovation</h4>
          <p>We constantly push boundaries to deliver cutting-edge solutions.</p>
        </div>
      </div>
      <div class="col-md-4 mb-4" data-aos="fade-up" data-aos-delay="200">
        <div class="text-center">
          <i class="bi bi-people text-primary" style="font-size: 3rem;"></i>
          <h4 class="mt-3">Collaboration</h4>
          <p>We believe in the power of teamwork and diverse perspectives.</p>
        </div>
      </div>
      <div class="col-md-4 mb-4" data-aos="fade-up" data-aos-delay="300">
        <div class="text-center">
          <i class="bi bi-graph-up text-primary" style="font-size: 3rem;"></i>
          <h4 class="mt-3">Excellence</h4>
          <p>We strive for the highest standards in everything we do.</p>
        </div>
      </div>
    </div>
  </div>
</section>

<!-- Team Expertise Section -->
<section id="team-expertise" class="py-5 bg-light">
  <div class="container" data-aos="fade-up">
    <h2 class="text-center mb-5">Our Collective Expertise</h2>
    <div class="row justify-content-center">
      <div class="col-lg-8">
        <div class="progress mb-4" data-aos="fade-up" data-aos-delay="100">
          <div class="progress-bar" role="progressbar" style="width: 95%;" aria-valuenow="95" aria-valuemin="0" aria-valuemax="100">Data Science & Analytics</div>
        </div>
        <div class="progress mb-4" data-aos="fade-up" data-aos-delay="200">
          <div class="progress-bar" role="progressbar" style="width: 90%;" aria-valuenow="90" aria-valuemin="0" aria-valuemax="100">IT Consulting & Solutions</div>
        </div>
        <div class="progress mb-4" data-aos="fade-up" data-aos-delay="300">
          <div class="progress-bar" role="progressbar" style="width: 85%;" aria-valuenow="85" aria-valuemin="0" aria-valuemax="100">Business Strategy</div>
        </div>
        <div class="progress mb-4" data-aos="fade-up" data-aos-delay="400">
          <div class="progress-bar" role="progressbar" style="width: 80%;" aria-valuenow="80" aria-valuemin="0" aria-valuemax="100">Project Management</div>
        </div>
      </div>
    </div>
  </div>
</section>

<!-- Join Our Team Section -->
<section id="join-team" class="py-5">
  <div class="container" data-aos="fade-up">
    <div class="row align-items-center">
      <div class="col-lg-6" data-aos="fade-right" data-aos-delay="100">
        <h2 class="mb-4">Join Our Growing Team</h2>
        <p>We're always looking for talented individuals who are passionate about technology and innovation. If you think you'd be a great fit for our team, we'd love to hear from you!</p>
        <a href="/careers" class="btn btn-primary">View Open Positions</a>
      </div>
      <div class="col-lg-6" data-aos="fade-left" data-aos-delay="200">
        <img src="lib/img/team-culture.jpg" alt="Team Culture" class="img-fluid rounded shadow">
      </div>
    </div>
  </div>
</section>

<!-- Call to Action Section -->
<section id="cta" class="cta py-5 bg-primary text-white">
  <div class="container" data-aos="zoom-in">
    <div class="text-center">
      <h3>Ready to Work with Our Expert Team?</h3>
      <p>Let's collaborate to bring your vision to life and drive your business forward.</p>
      <a class="btn btn-light btn-lg" href="/contact">Get in Touch Today</a>
    </div>
  </div>
</section>